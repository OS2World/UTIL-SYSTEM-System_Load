DLGTEMPLATE IDD_DSPROPERTIES1
{
  DIALOG  "CPU, page 1", IDD_DSPROPERTIES1, 0, 0, 240, 146,
          FS_NOBYTEALIGN | NOT FS_DLGBORDER
          PRESPARAMS PP_FONTNAMESIZE, "9.WarpSans"
  {
    CONTROL "Please, install CPUTEMP.WPI to display temperature of each CPU.",
            IDD_ST_NEED_DRIVER, 5, 135, 155, 8, WC_STATIC,
            SS_TEXT | DT_LEFT | DT_BOTTOM
          PRESPARAMS PP_FONTNAMESIZE, "8.WarpSans"

    CONTROL "Temperature", IDD_GB_TEMPERATURE, 5, 80, 155, 50, WC_STATIC,
            SS_GROUPBOX | DT_LEFT | DT_TOP | DT_MNEMONIC | WS_VISIBLE
    CONTROL "~Celsius (øC)", IDD_RB_TEMP_C, 10, 113, 58, 8, WC_BUTTON,
            BS_AUTORADIOBUTTON | WS_VISIBLE | WS_GROUP | WS_TABSTOP
    CONTROL "~Fahrenheit (øF)", IDD_RB_TEMP_F, 10, 104, 58, 8, WC_BUTTON,
            BS_AUTORADIOBUTTON | WS_VISIBLE
    CONTROL "~ACPI Thermal Zone pathname:", IDD_ST_PATHNAME, 12, 93, 100, 8, WC_STATIC,
            SS_TEXT | DT_LEFT | DT_TOP | DT_MNEMONIC | WS_VISIBLE
    CONTROL "",  IDD_CB_PATHNAME, 12, 56, 100, 37, WC_COMBOBOX,
            CBS_DROPDOWN | WS_GROUP | WS_TABSTOP | WS_VISIBLE
    CONTROL "~Test", IDD_PB_TEST, 115, 84, 40, 10, WC_BUTTON,
            BS_PUSHBUTTON | WS_VISIBLE | WS_TABSTOP

    CONTROL "~Show real frequency", IDD_CB_REALFREQ, 5, 67, 150, 8, WC_BUTTON,
            BS_AUTOCHECKBOX | WS_VISIBLE | WS_GROUP | WS_TABSTOP

    CONTROL "Background", IDD_ST_BGCOL, 15, 50, 50, 10, WC_STATIC,
            SS_TEXT | DT_LEFT | DT_TOP | WS_VISIBLE | WS_TABSTOP
    CONTROL "Border", IDD_ST_BORDERCOL, 70, 50, 50, 10, WC_STATIC,
            SS_TEXT | DT_LEFT | DT_TOP | WS_VISIBLE | WS_TABSTOP

    CONTROL "Graph time", IDD_GB_GRAPHTIME, 5, 5, 155, 40, WC_STATIC,
            SS_GROUPBOX | DT_LEFT | DT_TOP | DT_MNEMONIC | WS_VISIBLE | WS_GROUP
    CONTROL "Slider", IDD_SLID_TIME, 12, 13, 105, 24, WC_SLIDER, 
            SLS_HORIZONTAL | SLS_BOTTOM | SLS_HOMELEFT | SLS_BUTTONSRIGHT | SLS_PRIMARYSCALE1 | WS_VISIBLE | WS_GROUP | WS_TABSTOP
