            SS_TEXT | DT_LEFT | DT_TOP | WS_VISIBLE

    CONTROL "Graph colors", IDD_GB_COLORS, 162, 5, 45, 40, WC_STATIC,
            SS_GROUPBOX | DT_LEFT | DT_TOP | DT_MNEMONIC | WS_VISIBLE | WS_GROUP
    CONTROL "IRQ", IDD_ST_IRQCOL, 167, 9, 34, 10, WC_STATIC,
            SS_TEXT | DT_LEFT | DT_TOP | WS_VISIBLE | WS_TABSTOP
  }
}

DLGTEMPLATE IDD_SUCCESSTEMP
{
  DIALOG  "Success", IDD_SUCCESSTEMP, 0, 0, 170, 50, FS_DLGBORDER,
          FCF_TITLEBAR | FCF_SYSMENU | FCF_SCREENALIGN
          PRESPARAMS PP_FONTNAMESIZE, "9.WarpSans"
  {
    CONTROL "Temperature determined successfully:", IDD_ST_TEMP,
            5, 38, 100, 10, WC_STATIC,
            SS_TEXT | DT_LEFT | DT_BOTTOM | WS_VISIBLE | WS_TABSTOP
            PRESPARAMS PP_FONTNAMESIZE, "9.WarpSans Bold"
    CONTROL "", IDD_EF_TEMP, 110, 38, 20, 7, WC_ENTRYFIELD,
            ES_LEFT | WS_VISIBLE | WS_TABSTOP | ES_READONLY
            PRESPARAMS PP_BACKGROUNDCOLOR 0x0000FF00
            PRESPARAMS PP_FONTNAMESIZE, "9.WarpSans Bold"
    CONTROL "Please send e-mail to", IDD_ST_EMAIL,
            5, 28, 60, 10, WC_STATIC,
            SS_TEXT | DT_LEFT | DT_BOTTOM | WS_VISIBLE | WS_TABSTOP
    CONTROL "digi@frs.snc.ru", IDD_EF_EMAIL, 65, 28, 50, 7, WC_ENTRYFIELD,
            ES_LEFT | WS_VISIBLE | WS_TABSTOP | ES_READONLY 
            PRESPARAMS PP_BACKGROUNDCOLOR 0x0000FF00
    CONTROL "following line:", IDD_ST_PATHNAME,
            5, 19, 60, 10, WC_STATIC,
            SS_TEXT | DT_LEFT | DT_BOTTOM | WS_VISIBLE | WS_TABSTOP
    CONTROL "", IDD_EF_PATHNAME, 65, 19, 96, 7, WC_ENTRYFIELD,
            ES_LEFT | WS_VISIBLE | WS_TABSTOP | ES_READONLY 
            PRESPARAMS PP_BACKGROUNDCOLOR SYSCLR_MENUHILITEBGND
    CONTROL "~Close", IDD_PB_CLOSE, 4, 3, 40, 10, WC_BUTTON,
            BS_PUSHBUTTON | BS_DEFAULT | WS_VISIBLE | WS_TABSTOP
  }
}

